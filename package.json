    ]
  },
  "devDependencies": {
    "@types/node": "^24.0.13",
    "n8n-workflow": "^1.82.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {}
}

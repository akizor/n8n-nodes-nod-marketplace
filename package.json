{
  "name": "n8n-nodes-nod-marketplace",
  "version": "1.0.4",
  "description": "n8n community node for interacting with the NOD Marketplace API.",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://github.com/akizor/n8n-nodes-nod-marketplace#readme",
  "author": {
    "name": "Daniel P",
    "email": "daniel.placinta@yahoo.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akizor/n8n-nodes-nod-marketplace.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "nodes/NodMarketplace.node.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch"
  },
  "files": [
    "nodes"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
